{
  "name": "@status/codes",
  "version": "0.0.0-development",
  "description": "A collection of status code enums",
  "main": "index.js",
  "scripts": {
    "tslint": "tslint",
    "commit": "git-cz",
    "test": "mocha",
    "prebuild": "rimraf build",
    "build": "tsc",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "coverage:test": "nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "apache",
    "auth0",
    "cloudflare",
    "codes",
    "ftp",
    "http",
    "iis",
    "mocha",
    "mongodb",
    "nginx",
    "node",
    "status",
    "irc"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^8.0.1",
    "chai": "^4.2.0",
    "codecov": "^3.0.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.2.3",
    "mocha": "^8.1.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "semantic-release-cli": "^5.2.1",
    "source-map-support": "^0.5.10",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/status-codes.git"
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  }
}
