{
  "name": "@status/codes",
  "version": "0.0.0-development",
  "description": "A collection of status code enums",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:declarations": "tsc -d",
    "tslint": "tslint",
    "commit": "git-cz",
    "test": "mocha --compilers ts:ts-node/register ./test/*.spec.ts",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "coverage:test": "nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "status",
    "codes",
    "ftp",
    "http",
    "iis",
    "cloudflare",
    "mongodb",
    "nginx",
    "apache",
    "node"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "codecov": "^3.0.4",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.0",
    "source-map-support": "^0.5.10",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/status-codes.git"
  }
}
